{
  "name": "a2d3",
  "version": "1.0.0-rc.5",
  "description": "AngularJS chart directives and other integrations for D3.js",
  "authors": [
    "Chris Nicola <chnicola@gmail.com>"
  ],
  "homepage": "http://wealthbar.github.io/a2d3",
  "repository": {
    "type": "git",
    "url": "https://github.com/wealthbar/a2d3.git"
  },
  "bugs": {
    "url": "https://github.com/wealthbar/a2d3/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/",
    "src/angularD3/"
  ],
  "scripts": {
    "clean": "rimraf node_modules doc typings && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "dist": "webpack --config webpack.build.js",
    "e2e": "protractor",
    "test": "karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 4000",
    "postinstall": "typings install",
    "prepublish": "npm run dist"
  },
  "dependencies": {
    "@angular/core": "2.0.0-rc.3",
    "d3": "^3.5.16",
    "lodash.clonedeep": "^4.3.2"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "bootstrap-loader": "^1.0.1",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "es6-shim": "^0.35.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "jquery": "^3.0.0",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.4.4",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "3.2.1",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "ts-loader": "0.8.0",
    "tslint": "^3.10.2",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1",
    "zone.js": "^0.6.12"
  }
}
